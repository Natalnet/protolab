            $('#profResponsavelDiv').hide();
            $('#emailProfDiv').hide();
            $('#departamentoDiv').show();
            $('#tipoDeServicoDiv').show();
            $('#quantidadeDiv').show();
            $('#configPcbDiv').show();
            $('#arquivoDiv').show();
            $('#quantidadeDiv').hide();
            $('#configPcbDiv').hide();
            $('#arquivoDiv').hide();
            $('#informacoesBottom').show();
        }
        function redirectHome() {
            let irParaHome;
            if (confirm("Você deseja realmente cancelar seu formulário e ir para página principal?")) {
                window.location.href = "/";
            }
        }
        function exibirDiscente() {
            $('#solicitanteDiv').show();
            $('#emailSolicitanteDiv').show();
            $('#nomeProjetoDiv').show();
            $('#profResponsavelDiv').show();
            $('#emailProfDiv').show();
            $('#departamentoDiv').show();
            $('#tipoDeServicoDiv').show();
            $('#quantidadeDiv').show();
            $('#configPcbDiv').show();
            $('#arquivoDiv').show();
            $('#quantidadeDiv').hide();
            $('#configPcbDiv').hide();
            $('#arquivoDiv').hide();
            $('#informacoesBottom').show();
        }
        function ocultarAll() {
            $('#quantidadeDiv').hide();
            $('#configPcbDiv').hide();
            $('#arquivoDiv').hide();
            $('#solicitanteDiv').hide();
            $('#emailSolicitanteDiv').hide();
            $('#nomeProjetoDiv').hide();
            $('#profResponsavelDiv').hide();
            $('#emailProfDiv').hide();
            $('#departamentoDiv').hide();
            $('#tipoDeServicoDiv').hide();
            $('#quantidadeDiv').hide();
            $('#configPcbDiv').hide();
            $('#arquivoDiv').hide();
            $('#gifLoad').hide();
            $('#informacoesBottom').hide();
        }
        function mudancaTipoProcesso(valor) {
            if (valor === "impressao3d") {
                $('#quantidadeDiv').show();
                $('#configPcbDiv').hide();
                $('#arquivoDiv').show();
                $('#labelAviso').empty();
                $('#labelAviso').append(`Só serão aceitos arquivos com a extensão ".stl".`);
            } else if (valor === "impressaoPCB") {
                $('#quantidadeDiv').show();
                $('#configPcbDiv').show();
                $('#arquivoDiv').show();
                $('#labelAviso').empty();
                $('#labelAviso').append(`Você só poderá mandar arquivos geber ou com a extensão ".zip".`);
            } else {
                $('#quantidadeDiv').hide();
                $('#configPcbDiv').hide();
                $('#arquivoDiv').hide();
                $('#labelAviso').empty();
            }
        }
        function reexibirForm(vinculo){
            if (vinculo === "Discente") {
                exibirDiscente();
            } else if (vinculo === "Docente") {
                exibirDocente();
            } else if (vinculo === "Servidor") {
                exibirServidor();
            } else if (vinculo === "Externo") {
                exibirExterno();
            }
        }
        $(document).ready(function () {
            ocultarAll();
            $('#tipoProcesso').on('change', function () {
                mudancaTipoProcesso(this.value);
            });
            $('#tipoVinculo').on('change', function () {
                if (this.value === "Discente") {
                    exibirDiscente();
                } else if (this.value === "Docente") {
                    exibirDocente();
                } else if (this.value === "Servidor") {
                    exibirServidor();
                } else if (this.value === "Externo") {
                    exibirExterno();
                } else {
                    ocultarAll();
                }
            });
            $('#formProcesso').submit(function (event) {
                event.preventDefault();
                let vinculo = $('#tipoVinculo').val();
                let solicitante = $('#solicitante').val();
                let emailSolicitante = $('#emailSolicitante').val();
                let nomeProjeto = $('#nomeProjeto').val();
                let profResponsavel = $('#profResponsavel').val();
                let emailProf = $('#emailProf').val();
                let departamento = $('.departamento').val();
                let tipoServico = $('#tipoProcesso').val();
                let quantidade = $('#quantidade').val();
                let configPcb = $('#configPcb').val();
                let arquivo = $("#arquivo")[0].files[0];
                let formData = new FormData();

                // POPULANDO FORM //
                formData.append('vinculo', vinculo);
                formData.append('solicitante', solicitante);
                formData.append('emailSolicitante', emailSolicitante);
                formData.append('nomeProjeto', nomeProjeto);
                formData.append('profResponsavel', profResponsavel);
                formData.append('emailProfResponsavel', emailProf);
                formData.append('tipoServico', tipoServico);
                formData.append('quantidade', quantidade);
                formData.append('configPcb', configPcb);
                formData.append('arquivo', arquivo);
                formData.append('departamento', departamento);
                // POPULANDO FORM //
                ocultarAll();
                $('#divVinculo').hide();
                $('#gifLoad').show();

                $.ajax({
                    type: "POST",
                    enctype: 'multipart/form-data',
                    url: "/novo-processo",
                    data: formData,
                    processData: false,
                    contentType: false,
                    cache: false,
                    timeout: 600000,
                    success: function (data) {
                        if (data.erroForm){
                            reexibirForm($('#tipoVinculo').val());
                            mudancaTipoProcesso($('#tipoProcesso').val());
                            $('#gifLoad').hide();
                            showErrosForm(data);
                        }else if(data.statusErroFile){
                            reexibirForm($('#tipoVinculo').val());
                            mudancaTipoProcesso($('#tipoProcesso').val());
                            $('#gifLoad').hide();
                            showErrosFile(data);
                        }else if(data.status){
                            ocultarAll();
                            $('#divVinculo').hide();
                            $('#textoCabecalho').empty();
                            $('#textoCabecalho').append("Envio realizado com sucesso");
                            $('#erros').empty();
                            let texto = `<h2 style="text-align: justify;">O seu processo foi criado, anote ou guarde o seguinte código: <strong style="color:#B71C1C">${data.codigo}</strong> ele será o principal meio de comunicação entre o PROTOLAB e você, com ele você poderá mandar mensagens para exclarecer dúvidas ou fazer um acompanhamento mais detalhado do seu processo. Esse código foi enviado para o seu e-mail: <strong style="color:#1B5E20">${data.email}.</strong></h2>`
                            $('#erros').append(texto);
                        }
                    },
                    error: function (e) {
                        console.log("ERROR : ", e);
                    }
                });
            });
        });
    </script>
