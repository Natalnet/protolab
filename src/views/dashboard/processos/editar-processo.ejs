<!doctype html>
<html lang="pt-br" dir="ltr">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <meta http-equiv="Content-Language" content="pt-br" />
    <meta name="msapplication-TileColor" content="#2d89ef">
    <meta name="theme-color" content="#4188c9">
    <meta name="apple-mobile-web-app-status-bar-style" content="black-translucent" />
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="mobile-web-app-capable" content="yes">
    <meta name="HandheldFriendly" content="True">
    <meta name="MobileOptimized" content="320">
    <link rel="icon" href="/static/img/favicon.ico" type="image/x-icon" />
    <link rel="shortcut icon" type="image/x-icon" href="/static/img/favicon.ico" />

    <title>Processo
        <%=buscaProcesso.codigo%> - Laboratório de Prototipagem</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,300i,400,400i,500,500i,600,600i,700,700i&amp;subset=latin-ext">
    <script src="/static/assets/js/require.min.js"></script>
    <script>
        requirejs.config({
            baseUrl: '/static/'
        });
    </script>
    <!-- Dashboard Core -->
    <link href="/static/assets/css/dashboard.css" rel="stylesheet" />
    <script src="/static/assets/js/dashboard.js"></script>
    <!-- c3.js Charts Plugin -->
    <link href="/assets/plugins/charts-c3/plugin.css" rel="stylesheet" />
    <script src="/assets/plugins/charts-c3/plugin.js"></script>
    <!-- Google Maps Plugin -->
    <link href="/static/assets/plugins/maps-google/plugin.css" rel="stylesheet" />
    <script src="/static/assets/plugins/maps-google/plugin.js"></script>
    <!-- Input Mask Plugin -->
    <script src="/static/assets/plugins/input-mask/plugin.js"></script>

</head>

<body class="">
    <div class="page">
        <div class="page-main">
            <div class="header py-4">
                <div class="container">
                    <div class="d-flex">
                        <a class="header-brand" href="/dashboard">
                            <img src="/static/img/logo.svg" class="header-brand-img" alt="tabler logo">
                        </a>
                        <div class="d-flex order-lg-2 ml-auto">
                            <div class="dropdown">
                                <a href="#" class="nav-link pr-0 leading-none" data-toggle="dropdown">
                                    <span class="avatar" style="background-image: url(/get-img-perfil/<%=emailUser%>)"></span>
                                    <span class="ml-2 d-none d-lg-block">
                                        <span class="text-default">
                                            <%=nome%>
                                        </span>
                                        <small class="text-muted d-block mt-1">
                                            <%=tipoUsuario%>
                                        </small>
                                    </span>
                                </a>
                                <div class="dropdown-menu dropdown-menu-right dropdown-menu-arrow">
                                    <a class="dropdown-item" href="#">
                                        <i class="dropdown-icon fe fe-user"></i> Perfil
                                    </a>
                                    <div class="dropdown-divider"></div>
                                    <a class="dropdown-item" href="/dashboard/sair">
                                        <i class="dropdown-icon fe fe-log-out"></i> Sair
                                    </a>
                                </div>
                            </div>
                        </div>
                        <a href="#" class="header-toggler d-lg-none ml-3 ml-lg-0" data-toggle="collapse" data-target="#headerMenuCollapse">
                            <span class="header-toggler-icon"></span>
                        </a>
                    </div>
                </div>
            </div>
            <% include ../components/navbar %>
                <div class="my-3 my-md-5">
                    <div class="container">
                        <div class="page-header"></div>
                        <div class="row row-cards row-deck">
                            <div class="col-12">
                                <div class="card">
                                    <div class="card-header">
                                        <h3 class="card-title">Processo
                                            <%=buscaProcesso.codigo%>
                                        </h3>
                                    </div>
                                    <div class="card-body p-5">
                                        <div class="table-responsive">
                                            <table class="table card-table table-vcenter text-nowrap">
                                                <thead>
                                                    <tr style="text-align: center">
                                                        <th></th>
                                                        <th></th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>Finalizado: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <%if(!buscaProcesso.finalizado){%>
                                                                O processo está em andamento
                                                                <%}else{%>
                                                                    O processo foi finalizado
                                                                    <%}%>
                                                        </td>
                                                    </tr>

                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>Pausado: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <%if(!buscaProcesso.Pausado){%>
                                                                O processo não está pausado
                                                                <%}else{%>
                                                                    O processo está pausado
                                                                    <%}%>
                                                        </td>
                                                    </tr>

                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>Vinculo: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <input type="text" class="form-control" value="<%=buscaProcesso.vinculo%>">
                                                        </td>
                                                    </tr>

                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>Solicitante: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <input type="text" class="form-control" value="<%=buscaProcesso.solicitante%>">
                                                        </td>
                                                    </tr>
                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>E-mail do solicitante: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <input type="text" class="form-control" value="<%=buscaProcesso.emailSolicitante%>">
                                                        </td>
                                                    </tr>
                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>Professor responsável: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <input type="text" class="form-control" value="<%=buscaProcesso.profResponsavel%>">
                                                        </td>
                                                    </tr>
                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>E-mail Professor responsável: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <input type="text" class="form-control" value="<%=buscaProcesso.emailProfResponsavel%>">
                                                        </td>
                                                    </tr>
                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>Nome Projeto: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <input type="text" class="form-control" value="<%=buscaProcesso.nomeProjeto%>">
                                                        </td>
                                                    </tr>
                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>Departamento: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <input type="text" class="form-control" value="<%=buscaProcesso.departamento%>">
                                                        </td>
                                                    </tr>
                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>Tipo de serviço: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <%if(buscaProcesso.tipoServico==="impressao3d"){%>
                                                                Impressão 3D
                                                            <%}else{%>
                                                                Impressão de PCB
                                                            <%}%>
                                                        </td>
                                                    </tr>
                                                    <tr style="text-align: left">
                                                        <td>
                                                            <strong>Quantidade: </strong>
                                                        </td>
                                                        <td style="text-align: left">
                                                            <input type="text" class="form-control" value="<%= buscaProcesso.quantidade %>">
                                                        </td>
                                                    </tr>
                                                    <%if(!(buscaProcesso.tipoServico==="impressao3d")){%>
                                                        <tr style="text-align: left">
                                                            <td>
                                                                <strong>Configuração da PCB: </strong>
                                                            </td>
                                                            <td style="text-align: left">
                                                                <%= buscaProcesso.configPcb %>
                                                            </td>
                                                        </tr>
                                                        <%}%>
                                                            <tr style="text-align: left">
                                                                <td>
                                                                    <strong>Codigo: </strong>
                                                                </td>
                                                                <td style="text-align: left">
                                                                    <%= buscaProcesso.codigo %>
                                                                </td>
                                                            </tr>
                                                            <tr style="text-align: left">
                                                                <td>
                                                                    <strong>Arquivo: </strong>
                                                                </td>
                                                                <td style="text-align: left">
                                                                    <%let nomeArquivo = buscaProcesso.urlArquivo%>
                                                                        <%nomeArquivo = nomeArquivo.replace(`./src/uploads/processos/${buscaProcesso.codigo}/`,"")%>
                                                                            <a href="/dashboard/baixar-processo/<%=buscaProcesso._id%>" class="btn btn-secondary btn-block">
                                                                                <%=nomeArquivo%>
                                                                            </a>
                                                                </td>
                                                            </tr>
                                                            <tr style="text-align: left">
                                                                <td>
                                                                    <strong>Data abertura: </strong>
                                                                </td>
                                                                <td style="text-align: left">
                                                                    <%= buscaProcesso.dataAbertura.toLocaleString()%>
                                                                </td>
                                                            </tr>
                                                            <%if(buscaProcesso.finalizado){%>
                                                                <tr style="text-align: left">
                                                                    <td>
                                                                        <strong>Data finalização: </strong>
                                                                    </td>
                                                                    <td style="text-align: left">
                                                                        <%=buscaProcesso.dataFechamento.toLocaleString()%>
                                                                    </td>
                                                                </tr>
                                                                <%}%>
                                                                    <%let cor = ""%>
                                                                        <%let mensagem=""%>
                                                                            <%let status=""%>
                                                                                
                                                </tbody>
                                                
                                            </table>
                                            
                                        </div>
                                        
                                    </div>
                                    <br>
                                    <div class="row">
                                        <div class="col-lg-8 offset-lg-2">
                                            <button class="btn btn-success">Salvar alterações</button>
                                        </div>

                                    </div>
                                    <br><br><br>
                                </div>
                                
                            </div>
                        </div>
                    </div>
                </div>
        </div>

        <footer class="footer">
            <div class="container">
                <div class="row align-items-center flex-row-reverse">
                    <div class="col-12 col-lg-auto mt-3 mt-lg-0 text-center">
                        Laboratório de Prototipagem. Universidade Federal do Rio Grande do Norte 2018.
                    </div>
                </div>
            </div>
        </footer>
    </div>
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>


    <script>
        $(document).ready(function () {
            $('.pausarProcesso').click(function () {
                let status = `<%=status%>`;
                let id = this.value;
                $.ajax({
                    type: "POST",
                    url: "/dashboard/realizar-atualizacao-status-processo/",
                    data: {
                        id,
                        status,
                    },
                    timeout: 600000,
                    success: function (data) {
                        if (data.success) {
                            location.reload();
                        }
                    },
                    error: function (e) {
                        console.log("ERROR : ", e);
                    }
                });


            });
        });
    </script>
</body>

</html>